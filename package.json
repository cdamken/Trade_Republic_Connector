{
  "name": "trade-republic-connector",
  "version": "1.0.0",
  "description": "Modern TypeScript Trade Republic API connector with support for 400+ assets",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:manual": "tsx scripts/test-manual.ts",
    "cli:login": "tsx scripts/cli.ts login",
    "explore:database": "node scripts/explore-database.js",
    "demo:assets": "tsx examples/asset-data-demo.ts",
    "demo:assets-simple": "tsx examples/simple-asset-demo.ts",
    "demo:assets-mock": "tsx examples/asset-data-mock-demo.ts",
    "demo:enhanced": "tsx examples/enhanced-data-collection.ts",
    "demo:portfolio": "tsx examples/portfolio-asset-collection.ts",
    "demo:complete": "tsx examples/complete-asset-collection.ts",
    "demo:robust": "tsx examples/robust-asset-collection.ts",
    "demo:comprehensive": "tsx examples/comprehensive-asset-discovery.ts",
    "demo:websocket": "tsx examples/websocket-asset-discovery.ts",
    "demo:realdata": "tsx examples/complete-account-data.ts",
    "extract:account": "tsx examples/real-account-extractor.ts",
    "auth:2fa": "tsx examples/real-2fa-auth.ts",
    "auth:fresh": "tsx examples/fresh-device-pairing.ts",
    "auth:pair": "tsx examples/real-device-pairing.ts",
    "debug:test": "tsx examples/debug-timeout-test.ts",
    "debug:timeout": "tsx examples/debug-with-timeout.ts",
    "discover:endpoints": "tsx examples/endpoint-discovery.ts",
    "assets:all": "tsx examples/get-all-assets.ts",
    "assets:websocket": "tsx examples/websocket-asset-collector.ts",
    "assets:bulk": "tsx examples/bulk-asset-discovery.ts",
    "auth:interactive": "tsx examples/interactive-real-auth.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "trade-republic",
    "trading",
    "api",
    "typescript",
    "finance",
    "broker",
    "websocket"
  ],
  "author": "Carlos Damken <carlos@damken.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/ui": "^3.2.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "prettier": "^3.5.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "axios": "^1.10.0",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.5.0",
    "winston": "^3.17.0",
    "ws": "^8.18.2"
  }
}
